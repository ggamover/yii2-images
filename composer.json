{
    "name": "ggamover/yii2-images",
    "description": "yii2 images module for storing images",
    "keywords": ["yii2", "images", "store", "images store", "yii2-images", "Costa Rico"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",

    "authors": [
        {
            "name": "Costa Rico",
            "email": "zetogo@gmail.com"
        },
        {
            "name": "Rob Rhyne",
            "email": "rob.rhyne@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "*",
        "abeautifulsite/simpleimage": "3.7.2",
        "ext-exif": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "codeception/specify": "*",
        "codeception/verify": "*"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "rico\\yii2images\\": ""
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
